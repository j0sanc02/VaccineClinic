@model IEnumerable<UniversityofLouisvilleVaccine.Models.EmailLookup>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VacID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VacName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lotn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sent)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VacID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VacName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lotn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sent)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
